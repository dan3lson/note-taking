<h1>My events</h1>
<table>
  <tr>
    <th>Subject</th>
    <th>Start</th>
    <th>End</th>
    <th>Body</th>
    <th>Attendees</th>
  </tr>
  <% @events.each do |event| %>
    <tr>
      <td><%= event['Subject'] %></td>
      <td><%= event['Start'] %></td>
      <td><%= event['End'] %></td>
      <td><%= strip_tags(event['Body']['Content']) %></td>
      <td><%= event['Attendees'] %></td>
    </tr>
  <% end %>
</table>
