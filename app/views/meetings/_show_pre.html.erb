<div class="card">
	<div class="pull-right" id="pre-note-btn-container-<%= meeting.id %>">
		<%= render "notes/note_crud_links",
			meeting: meeting,
			note_exists: meeting.has_pre_notes?,
			note_type: "Pre" %>
	</div>
	<p>
		<i class="fa fa-circle-o"></i>
		Pre Notes
	</p>
	<br>
	<p id="pre-note-body-<%= meeting.id %>">
		<% if meeting.has_pre_notes? %>
			<%= meeting.pre_note_content %>
		<% else %>
			<small class="medium-gray">Empty</small>
		<% end %>
	</p>
	<% if meeting.has_pre_notes? %>
		<br>
		<small class="medium-gray">
			Last updated:
			<%= time_ago_in_words(meeting.retrieve_note(1).updated_at) %> ago
		</small>
	<% end %>
</div>
